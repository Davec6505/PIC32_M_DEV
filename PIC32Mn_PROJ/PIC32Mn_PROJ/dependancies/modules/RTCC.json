{
  "RTCC": {
    "RTCCON": [
      {
        "name": "RTCOE",
        "mask": "0x00000001",
        "caption": "RTCC Output Enable bit",
        "values": [
          {
            "caption": "RTCC output is enabled",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "RTCC output is not enabled",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "HALFSEC",
        "mask": "0x00000002",
        "caption": "Half-Second Status bit",
        "values": [
          {
            "caption": "Second half period of a second",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "First half period of a second",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "RTCSYNC",
        "mask": "0x00000004",
        "caption": "Real-Time Clock Value Registers Read Synchronization bit",
        "values": [
          {
            "caption": "Real-time clock value registers can change while reading (due to a rollover ripple that results in an invalid data read). If the register is read twice and results in the same data",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Real-time clock value registers can be read without concern about a rollover ripple",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "RTCWREN",
        "mask": "0x00000008",
        "caption": "Real-Time Clock Value Registers Write Enable bit",
        "values": [
          {
            "caption": "Real-Time Clock Value registers can be written to by the user",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Real-Time Clock Value registers are locked out from being written to by the user",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "RTCCLKON",
        "mask": "0x00000040",
        "caption": "RTCC Clock Enable Status bit",
        "values": [
          {
            "caption": "RTCC Clock is actively running",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "RTCC Clock is not running",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "RTCOUTSEL",
        "mask": "0x00000180",
        "caption": "RTCC Output Data Select bits",
        "values": [
          {
            "caption": "Reserved",
            "name": "",
            "value": "0x3"
          },
          {
            "caption": "RTCC Clock is presented on the RTCC pin",
            "name": "",
            "value": "0x2"
          },
          {
            "caption": "Seconds Clock is presented on the RTCC pin",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Alarm Pulse is presented on the RTCC pin when the alarm interrupt is triggered",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "RTCCLKSEL",
        "mask": "0x00000600",
        "caption": "RTCC Clock Select bits",
        "values": [
          {
            "caption": "Reserved",
            "name": "",
            "value": "0x3"
          },
          {
            "caption": "Reserved",
            "name": "",
            "value": "0x2"
          },
          {
            "caption": "RTCC uses the external 32.768 kHz Secondary Oscillator (SOSC)",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "RTCC uses the internal 32 kHz oscillator (LPRC)",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "SIDL",
        "mask": "0x00002000",
        "caption": "Stop in Idle Mode bit",
        "values": [
          {
            "caption": "Disables RTCC operation when CPU enters Idle mode",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Continue normal operation when CPU enters Idle mode",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "ON",
        "mask": "0x00008000",
        "caption": "RTCC On bit",
        "values": [
          {
            "caption": "RTCC module is enabled",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "RTCC module is disabled",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "CAL",
        "mask": "0x03FF0000",
        "caption": "Real-Time Clock Drift Calibration bits",
        "values": [
          {
            "caption": "Maximum positive adjustment",
            "name": "",
            "value": "0x1ff"
          },
          {
            "caption": "Minimum positive adjustment",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "No adjustment",
            "name": "",
            "value": "0x0"
          },
          {
            "caption": "Minimum negative adjustment",
            "name": "",
            "value": "0x3ff"
          },
          {
            "caption": "Minimum negative adjustment",
            "name": "",
            "value": "0x200"
          }
        ]
      }
