{
  "CFG": {
    "CFGCON": [
      {
        "name": "TDOEN",
        "mask": "0x00000001",
        "caption": "TDO Enable for 2-Wire JTAG",
        "values": [
          {
            "caption": "2-wire JTAG protocol uses TDO",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "2-wire JTAG protocol does not use TDO",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "TROEN",
        "mask": "0x00000004",
        "caption": "Trace Output Enable bit",
        "values": [
          {
            "caption": "Enable trace outputs and start trace clock (trace probe must be present)",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Disable trace outputs and stop trace clock",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "JTAGEN",
        "mask": "0x00000008",
        "caption": "JTAG Port Enable bit",
        "values": [
          {
            "caption": "Enable the JTAG port",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Disable the JTAG port",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "ECCCON",
        "mask": "0x00000030",
        "caption": "Flash ECC Configuration bits",
        "values": [
          {
            "caption": "ECC and dynamic ECC are disabled (ECCCON bits are writable)",
            "name": "",
            "value": "0x3"
          },
          {
            "caption": "ECC and dynamic ECC are disabled (ECCCON bits are locked)",
            "name": "",
            "value": "0x2"
          },
          {
            "caption": "Dynamic Flash ECC is enabled (ECCCON bits are locked)",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Flash ECC is enabled (ECCCON bits are locked; disables word Flash writes)",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "IOANCPN",
        "mask": "0x00000080",
        "caption": null,
        "values": []
      },
      {
        "name": "USBSSEN",
        "mask": "0x00000100",
        "caption": "USB Suspend Sleep Enable bit(1)",
        "values": [
          {
            "caption": "USB PHY clock is shut down when Sleep mode is active",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "USB PHY clock continues to run when Sleep is active",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "PGLOCK",
        "mask": "0x00000800",
        "caption": "Permission Group Lock bit(1)",
        "values": [
          {
            "caption": "Permission Group registers are locked. Writes to PG registers are not allowed",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Permission Group registers are not locked. Writes to PG registers are allowed",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "PMDLOCK",
        "mask": "0x00001000",
        "caption": "Peripheral Module Disable bit(1)",
        "values": [
          {
            "caption": "Peripheral module is locked. Writes to PMD registers are not allowed",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Peripheral module is not locked. Writes to PMD registers are allowed",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "IOLOCK",
        "mask": "0x00002000",
        "caption": "Peripheral Pin Select Lock bit(1)",
        "values": [
          {
            "caption": "Peripheral Pin Select is locked. Writes to PPS registers are not allowed",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "Peripheral Pin Select is not locked. Writes to PPS registers are allowed",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "OCACLK",
        "mask": "0x00010000",
        "caption": "Output Compare Alternate Clock Selection bit(1)",
        "values": [
          {
            "caption": "Output Compare modules use an alternative Timer pair as their timebase clock",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "All Output Compare modules use Timer2/3 as their timebase clock",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "ICACLK",
        "mask": "0x00020000",
        "caption": "Input Capture Alternate Clock Selection bit(1)",
        "values": [
          {
            "caption": "Input Capture modules use an alternative Timer pair as their timebase clock",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "All Input Capture modules use Timer2/3 as their timebase clock",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "CPUPRI",
        "mask": "0x01000000",
        "caption": "CPU Arbitration Priority to SRAM When Servicing an Interrupt bit(1)",
        "values": [
          {
            "caption": "CPU gets High Priority access to SRAM",
            "name": "",
            "value": "0x1"
          },
          {
            "caption": "CPU uses Least Recently Serviced Arbitration (same as other initiators)",
            "name": "",
            "value": "0x0"
          }
        ]
      },
      {
        "name": "DMAPRI",
        "mask": "0x02000000",
        "caption": "DMA Read and DMA Write Arbitration Priority to SRAM bit(1)",
        "values": [
          {
            "caption": "DMA gets High Priority access to SRAM",
